<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    
    <bean id = "task01" class = "main.java.todo.Task">
        <property name = "title" value = "Cleaning"/>
        <property name = "description" value = "Don't forget to scrub the floors"/>
    </bean>
            
    <bean id = "project01" class = "main.java.todo.Project">
        <property name = "title" value = "Household"/>
        <property name = "description" value = "Trying to keep it clean"/>
    </bean>
    
</beans>
